{
  "name": "meetup-service",
  "runtime": "nodejs4.3",
  "description": "Meetup CRUD API",
  "customName": "$${functionName}",
  "customRole": false,
  "handler": "meetup-service/meetup-http.handler",
  "timeout": 10,
  "memorySize": 1024,
  "authorizer": {},
  "custom": {
    "excludePatterns": [],
    "optimize": {
      "exclude": [
        "aws-sdk"
      ],
      "disable": false
    }
  },
  "endpoints": [
    {
      "path": "/api/meetups",
      "method": "POST",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${jsonRestRequest}",
      "responses": "$${jsonRestResponsePost}"
    },
    {
      "path": "/api/meetups/{meetupId}",
      "method": "PUT",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${jsonRestRequest}",
      "responses": "$${jsonRestResponse}"
    },
    {
      "path": "/api/meetups/{meetupId}",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${jsonRestRequest}",
      "responses": "$${jsonRestResponse}"
    },
    {
      "path": "/api/meetups",
      "method": "GET",
      "type": "AWS",
      "authorizationType": "none",
      "authorizerFunction": false,
      "apiKeyRequired": false,
      "requestParameters": {},
      "requestTemplates": "$${jsonRestRequest}",
      "responses": "$${jsonRestResponse}"
    }
  ],
  "events": [],
  "environment": {
    "SERVERLESS_PROJECT": "${project}",
    "SERVERLESS_STAGE": "${stage}",
    "SERVERLESS_REGION": "${region}",
    "LOCAL_DDB_ENDPOINT": "${localDynamoDbEndpoint}"
  },
  "vpc": {
    "securityGroupIds": [],
    "subnetIds": []
  }
}