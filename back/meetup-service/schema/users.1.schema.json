{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "apiVersion": {
      "type": "integer",
      "enum": [
        1
      ]
    },
    "correlationId": {
      "type": "string"
    },
    "data": {
      "type": "object",
      "properties": {
        "version": {
          "type": "integer"
        },
        "userId": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "status": {
          "type": "string",
          "enum": [
            "NEW",
            "ACTIVE",
            "CLOSED"
          ]
        },
        "secret":{
          "type": "string"
        },
        "loginType":{
          "type": "string",
          "enum": [
            "FACEBOOK",
            "TWITTER",
            "GOOGLE",
            "CUSTOM"
          ]
        }

      }
    }
  }
}